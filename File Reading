def merge_MENO_MEOR():
    try:
        print("EOA_To_MENO")
        with open('EOA_To_MENO.csv', 'a', newline='') as f:
            writer = csv.writer(f)
            with open('EOA_To_MEOR.csv', 'r') as f:
                reader = csv.reader(f)
                next(reader, None)
                for row in reader:
                    writer.writerow(row)

        pass
    except Exception as e:
        print("Merge CSV Exception :-" + str(e))
